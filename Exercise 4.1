/* Write the function strrindex(s, t), which returns the position of the rightmost occurrence of t in s, or -1 if there is none. */
#include <stdio.h>
#include <string.h>

int strrindex(char s[], char t[]) {
    int i, j, k;
    int rightmost = -1;

    for (i = 0; s[i] != '\0'; i++) {
        for (j = i, k = 0; t[k] != '\0' && s[j] == t[k]; j++, k++)
            ;
        if (k > 0 && t[k] == '\0')
            rightmost = i;   // update with new match
    }
    return rightmost;
}

int main() {
    char s[] = "this is a test, test again";
    char t[] = "test";

    int pos = strrindex(s, t);
    if (pos != -1)
        printf("Rightmost occurrence of '%s' in '%s' is at index %d\n", t, s, pos);
    else
        printf("Not found\n");

    return 0;
}
